        "print(f\"F1 (pos): {f1_tuned:.4f}\\n\")\n",
        "print(\"Classification report:\\n\", cr_tuned)\n",
        "print(\"Confusion matrix [neg,pos] rows x [neg,pos] cols:\\n\", cm_tuned)\n"
